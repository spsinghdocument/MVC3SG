@using PresantationAccessLeyer.Models
@model CustomerList
           
<table>
<tr >
<td>

<img src="@Model.ImageUrl" width="44" height="44" /> </td>
<td align="left">CHALLAN OUT</td>
<td><div>  Challan No : @Model.chno </div> 

</div></td>
</tr>
</table>






<br />

<table cellpadding="2" cellspacing="2">
    <tr border="1" bgcolor="#777777" color="#ffffff">
        
        <td width="5%" align="center" style=" font-size:small;">
            S. No
        </td>
      @*  <td width="9%" align="center" style=" font-size:small;">
            Challan NO
        </td>*@
        
          <td width="22%" align="center" style=" font-size:small;">
          Sim No
        </td>
         
          <td width="15%" align="center" style=" font-size:small;">
           Contry
        </td>
         <td width="15%" align="center" style=" font-size:small;">
           Phone No
        </td>
         
         <td width="9%" align="center" style=" font-size:small;">
            Pin
        </td>
      
       <td width="13%" align="center" style=" font-size:small;">
           puk
        </td>
  
    </tr>
    @{
        var OddColor = "#DDEEEE";
        var evenColor = "#EEFFFF";
        var Odd = OddColor;
    }
  
    @foreach (Customer customer in Model)
    {
        
        <tr border="1" bgcolor="@Odd">
            <td style=" font-size:small;">@customer.sno
            </td>
          @*  <td style=" font-size:small;">@customer.billno
            </td>*@
           
            <td style=" font-size:small;">@customer.simno
            </td>
             <td style=" font-size:small;">@customer.country
            </td>
            <td style=" font-size:small;">@customer.PhoneNo
            </td>
           
             <td style=" font-size:small;">@customer.simcode
            </td>
            <td style=" font-size:small;">@customer.puk
            </td>
         
            
        </tr>
        if (Odd == OddColor)
        {
            Odd = evenColor;
        }
        else
        {
            Odd = OddColor;
        }

    }
</table>
