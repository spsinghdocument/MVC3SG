    function abc_tcode(abc) {

        txtfixdcharges.value = abc[0].FIXED == null ? "0" : abc[0].FIXED;
        txtrental.value = abc[0].RENTAL == null ? "0" : abc[0].RENTAL;
        txtotherchg.value = abc[0].OTHERSCHG == null ? "0" : abc[0].OTHERSCHG;
        var total = $('#txttotal').val();
        var tval = parseFloat(txtfixdcharges.value == "null" ? 0 : txtfixdcharges.value)
        + parseFloat(txtfixdcharges.value == "null" ? 0 : txtfixdcharges.value)
        + parseFloat(txtrental.value == "null" ? 0 : txtrental.value)
        + parseFloat(txtotherchg.value == "null" ? 0 : txtotherchg.value) + parseFloat(total == "null" ? 0 : total)
        txt_total.value = tval.toFixed(2);

        var val_withpercent = (parseFloat(tval) * 12.36) / 100;

        txtservice_tax.value = val_withpercent.toFixed(2);
        var v_totalval = parseFloat(val_withpercent.toFixed(2)) + parseFloat(tval.toFixed(2));
        txt_totalamt.value = v_totalval.toFixed(2);
    }



