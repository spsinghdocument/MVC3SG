<script  type="text/javascript">
    function INSERDATA() {
        debugger;

        var name = $('#txtproname').val();
        if (name == "") {
            alert("Please insert profile name ");
            return;
        }
        var a = $('#txt_a').val();

        var b = $('#txt_a1').val();
        var c = $('#txt_b').val();
        var d = $('#txt_c').val();
        var e = $('#txt_d').val();

        var f = $('#txt_e').val();
        var g = $('#txt_f').val();
        if (a == "SELECT" && a == "SELECT" || b == "SELECT" || c == "SELECT" || d == "SELECT" || e == "SELECT" || f == "SELECT" || g == "SELECT") {
            alert("Please insert profile name ");
            return;
        }

        $.post('/Bill/PROFILEDATA_VALIDATE', { PROFILE_NAME: name, A: a, B: b, C: c, D: d, E: e, F: f, G: g }, function (data) { chkfun(data); }).error(function (data) { chkfun(data); });
    }
    function chkfun(abc) {
        alert(abc);
        window.location.href = '@Url.Action("Bill_Index","Bill")';
    }
    function close_btn() {

        window.location.href = '@Url.Action("Bill_Index","Bill")';
    }
</script>



