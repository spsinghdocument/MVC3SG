    function chredioop() {

        if ($("#rdentry").is(":checked")) {
            
            $("#lavel1").text('EXCEL FILLE UPLOAD');
        }
        else if ($("#rdsearch").is(":checked")) {
            $("#lavel1").text('PDF FILLE UPLOAD');
        }

    }
    function validation() {
        var caf = $("#txtCaf").val();
        var Username = $("#txtUSER_NAME").val();
        var mobileNo = $("#txtMOBILENO").val();
        var date = $("#txtDATE").val();
        var billNo = $("#txtBILLNO").val();
        var country = $("#txtCOUNTRY").val();
        var agreementno = $("#txtAGREEMENT_NO").val();
        var EmailID = $("#txtEMAILID").val();
        var user = $("#txtUser").val();
        var billDate = $("#txtBILLDATE").val();
        var billPeriod = $("#txtBILLPERIOD").val();
        var dueDate = $("#txtDUEDATE").val();
        var address = $("#txtADDRESS").val();





        if (caf == "") {
            alert("Please Fill CAF ");
            return;
        }

        if (Username == "") {
            alert("Please Fill User name");
            return;
        }

        if (mobileNo == "") {
            alert("Please Fill mobile No");
            return;
        }

        $.post('/Billing_invoice/abc/', { caf: caf, Username: Username, mobileNo: mobileNo, date: date, billNo: billNo, country: country, agreementno: agreementno, EmailID: EmailID, user: user, billDate: billDate, billPeriod: billPeriod, dueDate: dueDate, address: address }, function (data) { abc(data); }).error(function (data) { abc(data); });
       


    }

    function abc(bca) {
        
        window.location.href = '@Url.Action("Index", "Bill_inv_REP")';

    } 
