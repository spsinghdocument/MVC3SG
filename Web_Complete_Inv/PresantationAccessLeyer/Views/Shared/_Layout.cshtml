        $(document).ready(function () { loadusertype(0); });

        function loadusertype(abc) {

            $.post('/Home/LoadUserType', null, function (data) { Usertype_v(data); }).error(function (data) { Usertype_v(data); });

        }
        function Usertype_v(abc) {

            $("#TariffUpload").hide();
            
            var ab = abc;
            if (abc == "ADMIN") {
                return;
            }
            else if (abc == "PARTNER") {
                return;
            }
            else if (abc == "EXECUTIVE") {
                $.post('/Home/SearchUserRoleName', null, function (data) { chkuser_NameRole(data); }).error(function (data) { chkuser_NameRole(data); });
            }
//            alert(abc);

        }

        function chkuser_NameRole(abc) {

            var ab = abc[0].RoleSetting;
            if (ab == "") {
                alert("Please Role Assign....");
                var url = '@Url.Action("Index", "Login")';
                document.location = url;
                return false;
            }
            $.post('/Home/PermissionFun', { strolename: ab }, function (data) { chkshow(data); }).error(function (data) { chkshow(data); });          
        }

        function chkshow(abc) {
          

            $("#setting").hide();
