    <script type="text/javascript">

       

        $(function () {

            $("#Panel1").mouseenter(function () {
                $("#Panel1").show();
                $("#Panel4").show();
                $("#Panel1").animate({
                    width: '200px'
                }, 'fast');

            });

            $("#Panel1").mouseleave(function () {
                if (!$("#Panel1").is(':animated')) {
                    $("#Panel4").hide();
                    $("#Panel1").animate({
                        width: '15px'
                    }, 'fast');
                }

            });

        });

        function log_out() {

            var url = '@Url.Action("Index", "Login")';
            document.location = url;
            return false;


        }


