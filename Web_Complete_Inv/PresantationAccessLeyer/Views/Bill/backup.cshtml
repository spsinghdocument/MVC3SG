            if (number == undefined) {
                number = 0;
            }
            else
                number = abc[cd][1];
            if (abc[cd][2] == undefined) {
                type = 0;
            }
            else
                type = abc[cd][2];
            if (abc[cd][3] == undefined) {
                duration = 0;
            }
            else
                duration = abc[cd][3];
            if (abc[cd][4] == undefined) {
                unit = 0;
            }
            else
