    function chkGetCountry(abc) {

        var data = "";
        data += "<option value=-1 >Select</option>";
        data += "<option value=0 >OTHER</option>";

        for (var it in abc) {
            data += "<option value=" + abc[it].CountryName + " >" +
                 abc[it].CountryName + "</option>";
        }

